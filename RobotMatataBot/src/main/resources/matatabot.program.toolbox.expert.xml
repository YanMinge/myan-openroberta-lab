<toolbox_set id="toolboxExpert" style="display: none">
  <category name="TOOLBOX_ACTION" svg="true">
    <category name="TOOLBOX_MOVE" svg="true">
      <block type="matatabotActions_motion_moving">
        <value name="POS">
          <block type="math_number">
            <field name="NUM">10.0</field>
          </block>
        </value>
      </block>
      <block type="matatabotActions_motion_whirl">
        <value name="ANGLE">
          <block type="math_number">
            <field name="NUM">90</field>
          </block>
        </value>
      </block>        
      <block type="matatabotActions_motion_wheel_speed">
        <value name="LEFT_SPEED">
          <block type="math_number">
            <field name="NUM">10.0</field>
          </block>
        </value>
        <value name="RIGHT_SPEED">
          <block type="math_number">
            <field name="NUM">10.0</field>
          </block>
        </value>
      </block>
      <block type="matatabotActions_motion_stop_moving"></block>
    </category>
    <category name="TOOLBOX_SOUND" svg="true">
      <block type="matatabotActions_sound_alto"></block>
      <block type="matatabotActions_sound_treble"></block>
      <block type="matatabotActions_sound_melody"></block>
      <block type="matatabotActions_sound_song"></block>
    </category>
    <category name="TOOLBOX_LIGHT" svg="true">
      <block type="matatabotActions_led_single_set2">
        <value name="COLOR">
          <block type="robColour_picker">
            <field name="COLOUR">#FF1493</field>
          </block>
        </value>
      </block>
      <block type="matatabotActions_led_single_set3">
        <value name="LED_INDEX">
          <block type="math_number">
            <field name="NUM">0</field>
          </block>
        </value>
        <value name="RED_VALUE">
          <block type="math_number">
            <field name="NUM">100</field>
          </block>
        </value>
        <value name="GREEN_VALUE">
          <block type="math_number">
            <field name="NUM">100</field>
          </block>
        </value>
        <value name="BLUE_VALUE">
          <block type="math_number">
            <field name="NUM">100</field>
          </block>
        </value>
      </block>
      <block type="matatabotActions_led_all_off">
    </category>
  </category>
  <category name="TOOLBOX_CONTROL" svg="true">
    <category name="TOOLBOX_DECISION" svg="true">
      <block type="robControls_if"/>
      <block type="robControls_ifElse"/>
    </category>
    <category name="TOOLBOX_LOOP" svg="true">
      <block type="controls_whileUntil"/>
      <block type="controls_repeat_ext">
        <value name="TIMES">
          <block type="math_number">
            <field name="NUM">10</field>
          </block>
        </value>
      </block>
      <block type="robControls_for">
        <value name="FROM">
          <block type="math_number">
            <field name="NUM">0</field>
          </block>
        </value>
        <value name="TO">
          <block type="math_number">
            <field name="NUM">5</field>
          </block>
        </value>
        <value name="BY">
          <block type="math_number">
            <field name="NUM">1</field>
          </block>
        </value>
      </block>
      <block type="controls_flow_statements"/>
    </category>
    <category name="TOOLBOX_WAIT" svg="true">
      <block type="robControls_wait"/>
      <block type="robControls_wait_time">
        <value name="WAIT">
          <block type="math_number">
            <field name="NUM">500</field>
          </block>
        </value>
      </block>   
      <block type="robControls_wait_for">
      <value name="WAIT0">
        <block type="logic_boolean">
          <field name="BOOL">FALSE</field>
        </block>
      </value>
    </block>
    </category>
  </category>
  <category name="TOOLBOX_LOGIC" svg="true">
    <block type="logic_compare"/>
    <block type="logic_operation"/>
    <block type="logic_negate"/>
    <block type="logic_boolean"/>
    <block type="logic_null"/>
  </category>
  <category name="TOOLBOX_MATH" svg="true">
    <block type="math_number"/>
    <block type="math_arithmetic"/>
    <block type="math_single"/>
    <block type="math_trig"/>
    <block type="math_constant"/>
    <block type="math_number_property"/>
    <block type="robMath_change">
      <value name="DELTA">
        <block type="math_number">
          <field name="NUM">1</field>
        </block>
      </value>
    </block>
    <block type="math_round"/>
    <block type="math_modulo"/>
    <block type="math_constrain">
      <value name="LOW">
        <block type="math_number">
          <field name="NUM">1</field>
        </block>
      </value>
      <value name="HIGH">
        <block type="math_number">
          <field name="NUM">100</field>
        </block>
      </value>
    </block>
    <block type="math_random_int">
      <value name="FROM">
        <block type="math_number">
          <field name="NUM">1</field>
        </block>
      </value>
      <value name="TO">
        <block type="math_number">
          <field name="NUM">100</field>
        </block>
      </value>
    </block>
    <block type="math_random_float"/>
  </category>
  <category name="TOOLBOX_TEXT" svg="true">
    <block type="text"/>
    <block type="text_comment"/>
    <block type="robText_join"/>
  </category>
  <category name="TOOLBOX_COLOUR" svg="true">
    <block type="robColour_picker">
      <field name="COLOUR">#FF1493</field>
    </block>
    <block type="robColour_picker">
      <field name="COLOUR">#800080</field>
    </block>
    <block type="robColour_picker">
      <field name="COLOUR">#4876FF</field>
    </block>
    <block type="robColour_picker">
      <field name="COLOUR">#00FFFF</field>
    </block>
    <block type="robColour_picker">
      <field name="COLOUR">#90EE90</field>
    </block>
    <block type="robColour_picker">
      <field name="COLOUR">#008000</field>
    </block>
    <block type="robColour_picker">
      <field name="COLOUR">#FFFF00</field>
    </block>
    <block type="robColour_picker">
      <field name="COLOUR">#FFA500</field>
    </block>
    <block type="robColour_picker">
      <field name="COLOUR">#FF0000</field>
    </block>
    <block type="robColour_picker">
      <field name="COLOUR">#FFFFFE</field>
    </block>
  </category>
  <category name="TOOLBOX_VARIABLE" custom="VARIABLE" svg="true"/>
  <category name="TOOLBOX_PROCEDURE" custom="PROCEDURE" svg="true"/>
</toolbox_set>
